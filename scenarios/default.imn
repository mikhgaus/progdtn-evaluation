node n10 {
    type wlan
    network-config {
	hostname wlan10
	!
	interface wireless
	 ipv6 address 2001:0::0/128
	 ip address 10.0.0.0/24
	!
	mobmodel
	coreapi
	basic_range
	!
    }
    custom-config {
	custom-config-id basic_range
	custom-command {3 3 9 9 9}
	config {
	range=275
	bandwidth=54000000
	jitter=0
	delay=20000
	error=0
	}
    }
    canvas c1
    iconcoords {449.0 285.0}
    labelcoords {449.0 317.0}
    interface-peer {e0 n1}
    interface-peer {e1 n6}
    interface-peer {e2 n5}
    interface-peer {e3 n4}
    interface-peer {e4 n3}
    interface-peer {e5 n2}
    interface-peer {e6 n7}
    interface-peer {e7 n8}
    interface-peer {e8 n9}
}

link l1 {
    nodes {n10 n1}
}

