node n1 {
    type router
    model mobile
    network-config {
	hostname n1
	!
	interface eth0
	 ipv6 address 2001:0::1/128
	 ip address 10.0.0.1/24
	!
    }
    canvas c1
    iconcoords {144.0 75.0}
    labelcoords {144.0 107.0}
    interface-peer {eth0 n10}
}

